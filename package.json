{
  "name": "ngx-daterangepicker-timepicker-material",
  "version": "16.0.2",
  "description": "Angular 16 date range picker (with material design theme)",
  "keywords": [
    "angular2",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "angular10",
    "angular11",
    "angular12",
    "angular13",
    "ng2",
    "daterangepicker",
    "timepicker",
    "material",
    "component",
    "directive",
    "daterange",
    "date",
    "dayjs",
    "picker"
  ],
  "homepage": "https://github.com/leonSouffes/ngx-daterangepicker",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "demo:build": "ng build --configuration production",
    "test": "./node_modules/.bin/ng test --watch=false",
    "lint": "ng lint",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.json",
    "gh-pages": "ng build --configuration production --base-href=\"/ngx-daterangepicker-material/\" && ngh --dir demo/dist",
    "build": "ng-packagr -p ng-package.json -c tsconfig.json",
    "build:schematics": "tsc -p ./tsconfig.schematics.json",
    "copy:schematics": "cpr schematics/dist dist/schematics && cp ./schematics/collection.json ./dist/schematics && cp ./schematics/ng-add/schema.json ./dist/schematics/ng-add",
    "postbuild": "npm run build:schematics && npm run copy:schematics"
  },
  "author": "Souffes L",
  "repository": {
    "type": "git",
    "url": "https://github.com/leonSouffes/ngx-daterangepicker"
  },
  "peerDependencies": {
    "dayjs": "^1.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.1.0",
    "@angular-eslint/eslint-plugin": "^16.1.0",
    "@angular-eslint/eslint-plugin-template": "^16.1.0",
    "@angular-eslint/schematics": "^16.1.0",
    "@angular-eslint/template-parser": "^16.1.0",
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/cli": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@schematics/angular": "^14.2.12",
    "@types/jasmine": "^3.10.11",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^14.18.54",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "angular-cli-ghpages": "^1.0.3",
    "core-js": "^2.6.12",
    "cpr": "^3.0.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.3.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "materialize-css": "^1.0.0",
    "ng-packagr": "^16.2.0",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "rimraf": "^2.6.1",
    "rxjs": "^6.6.7",
    "ts-node": "~8.3.0",
    "typescript": "^5.1.6",
    "zone.js": "^0.13.1"
  },
  "dependencies": {
    "dayjs": "^1.11.4",
    "tslib": "^2.0.0"
  },
  "schematics": "./schematics/collection.json"
}
